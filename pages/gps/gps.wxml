<!--pages/gps/gps.wxml-->
<!-- <text>pages/gps/gps.wxml</text> -->
<map  id="map" longitude="{{jd}}" latitude="{{wd}}"  style="width: 100%; height: 100vh;"  show-location></map>

<form bindsubmit="formSubmit" bindreset="formReset">
<view class="daka">

<view class="jl"><view class="img"><image src="/images/bar/gps.png"></image></view><input name="GPS_jl" value="{{jl}}" disabled="disabled"></input></view>
<view class="jl2" >距离打卡地</view>

<view class="jwd">
<view><text class="jd1" style="color: #777777;font-size:16px;display:inline-block;float:left; position:fixed;top:2;">经度</text><input  class="jd" name="GPS_jd" value="{{jd}}" disabled="disabled"></input></view>

<view class='underline'></view>

<view><text class="jd1" style="color: #777777;font-size:16px;display:inline-block;float:left; position:fixed;top:2; margin-top: 10px;">纬度</text><input  class="wd" name="GPS_wd" value="{{wd}}" disabled="disabled"></input></view>
</view>
</view>


<view class="wbtn">
 <button   hover-class="btn_hover" class="btn" disabled="{{disabled}}" type="{{btnstate}}"  form-type="submit" style="background-color: #4AB09C;color: white;">打  卡</button></view>
</form>

